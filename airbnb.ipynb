def currency_formatter(x, pos):
    """Formatiert die Achsenwerte in Tsd. € oder Mio. €."""
    if x >= 1e6:
        return f'{x*1e-6:1.1f} Mio. €'
    elif x >= 1e3:
        return f'{x*1e-3:1.0f} Tsd. €'
    return f'{x:1.0f} €' 

